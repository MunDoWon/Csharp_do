#사원정보
DESC emp;
#부서정보
DESC DEPT;
#사원급여정보
DESC SALGRADE;
#P80
SELECT * FROM EMP;
SELECT EMPNO,ENAME,DEPTNO
FROM EMP;
SELECT DEPTNO FROM EMP;
#중복제거
SELECT DISTINCT DEPENC FROM EMP;

SELECT DISTINCT JOB, DEPTNO FROM EMP
ORDER BY DEPTNO;

SELECT ALL JOB, DEPTNO FROM EMP;
#P.81 컬럼으로 연산을 할 수 있는가?
SELECT ENAME, SAL*12+COMM,COMM FROM EMP;
SELECT ENAME, SAL*12+NVL(COMM,0) AS 연봉,NVL(COMM,0) FROM EMP;
