using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Eunm
{
    internal class Program
    {
        //멤버 변수 
        enum DigitalResult { YES, NO,CANCEL, CONFIRM,OK } //0,1,2,3,4 순
        static void Main(string[] args)
        {
            Console.WriteLine(DigitalResult.YES);
            Console.WriteLine((int)DigitalResult.YES);  //인덱스 번호

            Console.WriteLine(DigitalResult.OK);
            Console.WriteLine((int)DigitalResult.OK);

        }
    }
}
