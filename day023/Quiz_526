using System;

namespace Quiz_526
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] array = { 11, 22, 33, 44, 55 };
            foreach (int a in array)
            {
                Action<int> action = (x) => Console.WriteLine(x * x);
                action(a);
            }

        }
    }
}
